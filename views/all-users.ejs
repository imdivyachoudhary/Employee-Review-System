<table class="display dataTable" id="all-users-table">
  <thead>
    <tr>
      <th>Employee</th>
      <th>Email-Id</th>
      <th>Role</th>
      <th>Edit Employee</th>
      <th>Remove Employee</th>
      <th>Assign Reviewers</th>
      <th>Reviewers</th>
    </tr>
  </thead>
  <tbody>
    <tr id="row-id">
      <td class="user-name">Name</td>
      <td class="user-email">Email</td>
      <td class="user-role">Role</td>
      <td><i class="fas fa-user-edit" style="color: #1255ca" data-bs-toggle="modal"
        data-bs-target="#modal" onclick="editUserForm(this)"></i></td>
      <td><i class="fas fa-user-times" style="color: #cf200c" onclick="removeUser(this)"></i></td>
      <td><i class="fas fa-user-plus" style="color: #02a0e3;" data-bs-toggle="modal"
        data-bs-target="#modal" onclick="assignReviewersForm(this)"></i></td>
      <td><i class="fas fa-eye" style="color: #038705;" onclick="goToPage(this,'/user/admin/view-reviewers')"></i></td>
    </tr>
  </tbody>
</table>
